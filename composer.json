{
    "require": {
        "born05/craft-assetusage": "^3.3.0",
        "craftcms/ckeditor": "^3.8.3",
        "craftcms/cms": "^4.0.0",
        "mmikkel/cp-clearcache": "^1.3.1",
        "mmikkel/cp-field-inspect": "^1.4.4",
        "mmikkel/retcon": "^2.7.5",
        "nystudio107/craft-imageoptimize": "^4.0.8",
        "nystudio107/craft-seomatic": "^4.0.49",
        "solspace/craft-freeform": "^5.2.1",
        "topshelfcraft/environment-label": "^4.0.3",
        "topshelfcraft/wordsmith": "^4.4.0",
        "verbb/cp-nav": "^4.0.12",
        "verbb/expanded-singles": "^2.0.5",
        "vlucas/phpdotenv": "^3.4.0"
    },
    "require-dev": {
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1.26"
        },
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.craftcms.com",
            "canonical": false
        }
    ]
}
